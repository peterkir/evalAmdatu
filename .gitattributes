# fix eol with LF for following text based files
.classpath   text eol=lf
.project     text eol=lf
.options     text eol=lf
.gitignore   text eol=lf
gradlew      text eol=lf
packageinfo  text eol=lf
LICENSE      text eol=lf
Makefile     text eol=lf
README       text eol=lf
p2.inf       text eol=lf
p2.index     text eol=lf
*.aj         text eol=lf ident
*.auth       text eol=lf
*.awk        text eol=lf
*.bnd        text eol=lf
*.bndrun     text eol=lf
*.bundles    text eol=lf
*.c          text eol=lf ident
